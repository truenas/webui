msgstr "序列号"

msgid "Serial Number"
msgstr "序列号"

msgid "Serial Port"
msgstr "串行端口"

msgid "Serial Speed"
msgstr "串行速度"

msgid "Series"
msgstr "系列"

msgid "Serve UDP NFS clients"
msgstr "伺服 UDP NFS 客户端"

msgid "Server Role"
msgstr "服务器角色"

