{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project":
  {
    "name": "freenas-material-ui"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "icons",
        "assets/images",
        "assets/customicons"
        ],
        "index": "index.html",
        "main": "main.ts",
        "polyfills": "polyfills.ts",
        "test": "test.ts",
        "tsconfig": "tsconfig.app.json",
        "testTsconfig": "tsconfig.spec.json",
        "prefix": "app",
        "styles": [
          "assets/styles/reset.css",
          "../node_modules/xterm/dist/xterm.css",
          "../node_modules/quill/dist/quill.core.css",
          "../node_modules/quill/dist/quill.snow.css",
          "../node_modules/dragula/dist/dragula.css",
          "../node_modules/angular-calendar/css/angular-calendar.css",
          "../node_modules/hopscotch/dist/css/hopscotch.min.css",
          "../node_modules/perfect-scrollbar/dist/css/perfect-scrollbar.css",
          "../node_modules/chartist/dist/chartist.min.css",
          "../node_modules/@swimlane/ngx-datatable/assets/icons.css",
          "../node_modules/c3/c3.min.css",
          "../node_modules/angular-tree-component/dist/angular-tree-component.css",
          
          "../node_modules/primeicons/primeicons.css",
          "../node_modules/primeng/resources/primeng.min.css",
          "assets/styles/themes/ix-blue.scss",
          "assets/styles/fn-styles.css",
          "assets/styles/charts.css",
          "assets/styles/egret_overrides.css",
          "assets/styles/material-reduction.css"
        ],
        "scripts": [

          "../node_modules/perfect-scrollbar/dist/js/perfect-scrollbar.js",
          "../node_modules/hopscotch/dist/js/hopscotch.min.js",
          "../node_modules/hammerjs/hammer.js",
          "vendor/Chart.min.js",
          "../node_modules/quill/dist/quill.min.js",
          "../node_modules/element-resize-detector/dist/element-resize-detector.min.js",
          "../node_modules/filesize/lib/filesize.js",
          "../node_modules/xterm/dist/xterm.js",
          "../node_modules/xterm/dist/addons/attach/attach.js",
          "../node_modules/xterm/dist/addons/fit/fit.js"

        ],
        "environmentSource": "environments/environment.ts",
        "environments":
        {
          "dev": "environments/environment.ts",
          "prod": "environments/environment.prod.ts"
        }
    }],
    "e2e":
    {
      "protractor":
      {
        "config": "./protractor.conf.js"
      }
    },
    "lint": [
      {
        "project": "src/tsconfig.app.json"
      },
      {
        "project": "src/tsconfig.spec.json"
      },
      {
        "project": "e2e/tsconfig.e2e.json"
      }],
      "test":
      {
        "karma":
        {
          "config": "./karma.conf.js"
        }
      },
      "defaults":
      {
        "styleExt": "css",
        "component":
        {}
      }
}
