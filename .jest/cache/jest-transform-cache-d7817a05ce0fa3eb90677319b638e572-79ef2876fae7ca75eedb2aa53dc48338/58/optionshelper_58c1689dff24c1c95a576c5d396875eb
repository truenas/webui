"use strict";
Object.defineProperty(exports, "__esModule", { value: true });
exports.mapToOptions = mapToOptions;
exports.findLabelsByValue = findLabelsByValue;
exports.generateOptionsRange = generateOptionsRange;
function mapToOptions(optionMap, translate) {
    return Array.from(optionMap.entries()).map(([value, label]) => ({ label: translate.instant(label), value }));
}
/**
 * @usage
 * valueToLabel(options)(value)
 */
function findLabelsByValue(options) {
    return (value) => {
        const selectedOption = options.find((option) => option.value === value);
        return selectedOption === null || selectedOption === void 0 ? void 0 : selectedOption.label;
    };
}
function generateOptionsRange(start, end) {
    return Array.from({ length: end - start + 1 }, (_, index) => {
        const value = start + index;
        return { label: String(value), value };
    });
}
