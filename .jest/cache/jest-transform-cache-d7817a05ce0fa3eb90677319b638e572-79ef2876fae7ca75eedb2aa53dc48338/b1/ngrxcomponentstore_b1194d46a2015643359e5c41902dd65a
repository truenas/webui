function debounceSync() {
  return (source) => new import_rxjs.Observable((observer) => {
    let actionSubscription;
    let actionValue;
    const rootSubscription = new import_rxjs.Subscription();
    rootSubscription.add(source.subscribe({
      complete: () => {
        if (actionSubscription) {
          observer.next(actionValue);
        }
        observer.complete();
      },
      error: (error) => {
        observer.error(error);
      },
      next: (value) => {
        actionValue = value;
        if (!actionSubscription) {
          actionSubscription = import_rxjs.asapScheduler.schedule(() => {
            observer.next(actionValue);
            actionSubscription = void 0;
          });
          rootSubscription.add(actionSubscription);
        }
      }
    }));
    return rootSubscription;
  });
}
