  }
  get queryParamMap() {
    var _a;
    (_a = this._queryParamMap) != null ? _a : this._queryParamMap = convertToParamMap(this.queryParams);
    return this._queryParamMap;
  }
  /** @docsNotRequired */
  toString() {
