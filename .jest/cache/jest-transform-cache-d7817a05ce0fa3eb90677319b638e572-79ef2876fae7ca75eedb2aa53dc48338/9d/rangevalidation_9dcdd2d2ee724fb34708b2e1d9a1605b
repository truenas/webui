Object.defineProperty(exports, "__esModule", { value: true });
exports.rangeValidator = rangeValidator;
exports.portRangeValidator = portRangeValidator;
function rangeValidator(min, max) {
    return function rangeValidate(control) {
        let regex;
        if (min === 0) {
            regex = /^(0|[1-9]\d*)$/;
        }
        else {
            regex = /^[1-9]\d*$/;
        }
        if (!control.value) {
            return null;
        }
        if (regex.test(control.value)) {
            const num = Number(control.value);
            if (num >= min) {
                if (max) {
                    if (num <= max) {
                        return null;
                    }
                }
                else {
                    return null;
                }
            }
        }
        return { range: true, rangeValue: { min, max } };
    };
}
function portRangeValidator() {
    return rangeValidator(1, 65535);
}
